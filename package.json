{
  "name": "cart-woocommerce",
  "description": "Gateway de pagamento Mercado Pago para WooCommerce",
  "scripts": {
    "build:js": "gulp",
    "lint": "gulp jshint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js,!*.min.js": [
      "npm run lint"
    ]
  },
  "version": "2.12.7",
  "repository": {
    "type": "git",
    "url": "https://github.com/mercadopago/cart-woocommerce"
  },
  "main": "gulpfile.js",
  "devDependencies": {
    "gulp": "^4.0.2",
    "gulp-cli": "^2.2.0",
    "gulp-jshint": "^2.1.0",
    "gulp-livereload": "^4.0.2",
    "gulp-rename": "^2.0.0",
    "gulp-uglify": "^3.0.2",
    "jshint": "^2.11.0",
    "jshint-stylish": "^2.2.1",
    "husky": "^4.2.3",
    "lint-staged": "^10.0.8"
  }
}
