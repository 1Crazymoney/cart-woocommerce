{
  "name": "mp-plugins/woocommerce-plugins-enablers",
  "description": "Repository that contains the base project for the Plugins Enablers initiative",
  "license": "proprietary",
  "authors": [
    {
      "name": "Mercado Pago SMB",
      "email": "pp_smb@mercadolivre.com"
    }
  ],
  "minimum-stability": "stable",
  "autoload": {
    "psr-4": {
      "MercadoPago\\CartWoocommerce\\": [
        "includes"
      ]
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MercadoPago\\CartWoocommerce\\Tests\\": [
        "tests"
      ]
    }
  },
  "scripts": {
    "lint": "./vendor/bin/phpcs -q --report=checkstyle --standard=PSR2 includes"
  },
  "require": {
    "php": ">=7.2"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.7"
  }
}
